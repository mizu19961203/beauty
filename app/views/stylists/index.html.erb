<h1>
<table>
  <thead>
    <tr>
      <td></td>
      <td>名前</td>
      <td>自己紹介</td>
      <td>プロフィール写真</td>
      <td>開始日</td>
      <td>終了日</td>
      <td></td>
    </tr>
  </thead>
  <tbody>
    <% @stylists.each do |stylist| %>
      <tr>
        <td><%= stylist.id %></td>
        <td><%= stylist.name %></td>
        <td><%= stylist.description %></td>
        <!-- <td><%= image_tag(stylist) %></td> -->
        <td><%= stylist.date_start %></td>
        <td><%= stylist.date_fin %></td>
        <td><%= link_to '詳細', stylist_path(stylist) %></td>
        <td><%= link_to '編集', edit_stylist_path(stylist) %></td>
        <td><%= link_to '削除', stylist_path(stylist), method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</h1>

